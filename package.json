  "type": "module",
  "repository": "git@github.com:Broadshield/github-action-pr-label-toggle.git",
  "author": "Jamie Nelson <jamie@wearsafe.com>",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=16.20"
  },
